{
  "name": "config-service",
  "version": "1.0.0",
  "description": "Service for managing configurations",
  "main": "index.js",
  "scripts": {
    "build": "npm run generate-docs && tsc",
    "generate-docs": "swagger-jsdoc-generator config.ts > apidoc.json",
    "start": "ts-node src/index.ts",
    "dev-start": "nodemon --exec ts-node -r dotenv/config ./src/index.ts",
    "start:debug": "ts-node --inspect=5858 --debug-brk --ignore false -r dotenv/config ./src/index.ts",
    "coverage": "nyc npm run test",
    "test": "cross-env TS_NODE_FILES=true ts-mocha --all â€”exit test/**/*.ts",
    "prepare": "husky install && husky set .husky/pre-commit \"npx lint-staged\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@edunxtv2/service-util": "^6.0.2",
    "@types/express": "^4.17.13",
    "chai": "^4.3.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "delay": "^4.4.1",
    "dotenv": "^8.2.0",
    "express-async-errors": "^3.1.1",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.3.2",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mocha": "^8.4.0",
    "mongoose": "^5.10.3",
    "node-exec-promise": "^1.0.2",
    "nyc": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^3.1.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^8.2.2",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.1.5",
    "@types/nodemon": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.6",
    "swagger-jsdoc-generator": "^1.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint"
  }
}
